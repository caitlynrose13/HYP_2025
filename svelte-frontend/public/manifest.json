{
  "manifest_version": 3,
  "name": "TLS Analyser",
  "version": "1.0",
  "description": "A Chrome extension built with Svelte to analyse a domains TLS configurations",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "tabs"],
  "host_permissions": [
    "http://127.0.0.1:8080/",
    "http://localhost:8080/"
  ],
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
